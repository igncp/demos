{
  "name": "demos",
  "private": true,
  "description": "Personal demos about data visualization",
  "version": "0.1.0",
  "author": "igncp",
  "dependencies": {
    "@types/d3": "^7.0.0",
    "@types/jquery": "^3.5.6",
    "@types/jqueryui": "^1.12.16",
    "@types/qunit": "^2.11.1",
    "@types/raphael": "^2.3.1",
    "@types/react-helmet": "^6.1.2",
    "@types/topojson-client": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "d3": "^7.0.0",
    "d3-axis": "^3.0.0",
    "d3-delaunay": "^6.0.2",
    "d3-scale": "^4.0.0",
    "eslint": "^7.29.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-react": "^7.24.0",
    "gatsby": "^3.6.2",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-gatsby-cloud": "^2.6.1",
    "gatsby-plugin-image": "^1.6.0",
    "gatsby-plugin-manifest": "^3.6.0",
    "gatsby-plugin-offline": "^4.6.0",
    "gatsby-plugin-react-helmet": "^4.8.0",
    "gatsby-plugin-sharp": "^3.6.0",
    "gatsby-plugin-stylus": "^3.8.0",
    "gatsby-source-filesystem": "^3.6.0",
    "gatsby-transformer-sharp": "^3.6.0",
    "highlight.js": "^11.1.0",
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "prismjs": "^1.24.1",
    "prop-types": "^15.7.2",
    "qunit": "^2.16.0",
    "raphael": "^2.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "topojson-client": "^3.1.0",
    "typescript": "^4.3.5",
    "typescript-coverage-report": "^0.6.0"
  },
  "devDependencies": {
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "2.2.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rm -rf public && npm run check:ts && gatsby build --prefix-paths && npm run ts-coverage:gh-pages",
    "build:gh-pages": "ROOT_PATH=/demos/ npm run build",
    "check:ci": "npm run build && npm run lint",
    "check:ts": "tsc --noEmit --project .",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "lint": "eslint --ext .js --ext .ts --ext .tsx .",
    "lint-fix": "npm run lint -- --fix",
    "print-js-demos": "find src/demos -type f | grep 'js$' | xargs -I{} wc -l {} | sort -n",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "echo 'Tests are written in the page: src/pages/testing.tsx'",
    "ts-coverage": "typescript-coverage-report",
    "ts-coverage:gh-pages": "npm run ts-coverage && mv coverage-ts public"
  },
  "typeCoverage": {
    "atLeast": 90
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/igncp/demos.git"
  }
}
