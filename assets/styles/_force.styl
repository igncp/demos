.force-chart
    path.link {
      fill: none;
      stroke: #666;
      stroke-width: 1.5px;
    }

    circle {
      fill: #9c9;
      stroke: #fff;
      stroke-width: 1.5px;
    }

    text {
      fill: #999;
      font: 10px sans-serif;
      pointer-events: none;
    }

    .node text { 
      font-size:12px;
      fill:#000;
    }