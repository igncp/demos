.fish-eye-chart
  text
    font 10px sans-serif
    text-shadow 1px 1px 1px #ccc

  .axis path, .axis line
    fill none
    stroke #eee
    shape-rendering crispEdges

  .background
    fill none
    pointer-events all

  .chart-title
    font-size 14px

  .pointer
    fill #7AAE61
    font-size 15px
    opacity 0
